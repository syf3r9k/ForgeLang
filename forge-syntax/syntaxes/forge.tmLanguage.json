{
    "scopeName": "source.forge",
    "patterns": [
      {
        "name": "keyword.control.forge",
        "match": "\\b(func|case|mut|if|elf|els|while|for|out|outln|inp|try|catch|always)\\b"
      },
      {
        "name": "constant.numeric.forge",
        "match": "\\b\\d+\\b"
      },
      {
        "name": "storage.type.forge",
        "match": "\\b(i8|i16|i32|i64|i128|u8|u16|u32|u64|u128|f32|f64|str|bool|arr)\\b"
      },
      {
        "name": "string.quoted.double.forge",
        "begin": "\"",
        "end": "\"",
        "patterns": [
          {
            "name": "constant.character.escape.forge",
            "match": "\\\\."
          }
        ]
      },
      {
        "name": "comment.line.double-slash.forge",
        "match": "//.*$"
      }
    ],
    "repository": {},
    "fileTypes": ["frg"],
    "name": "Forge",
    "uuid": "a1234567-89ab-cdef-0123-456789abcdef"
  }
  